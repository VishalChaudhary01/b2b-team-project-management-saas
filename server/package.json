{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev --respawn --require tsconfig-paths/register src/index.ts"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@enums": "dist/enums",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@validators": "dist/validators"
  },
  "keywords": [],
  "author": "Vishal Chaudhary",
  "license": "MIT",
  "description": "ExpressJS + TypeScript + MongoDB Server",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "module-alias": "^2.2.3",
    "mongoose": "^8.13.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.10.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
